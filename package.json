{
  "name": "mmcesim",
	"displayName": "mmCEsim",
	"description": "Millimeter Wave Channel Estimation Simulation",
	"icon": "https://img.mmcesim.org/logo/mmCEsim_logo_256.png",
	"publisher": "mmcesim",
	"version": "0.0.1",
	"author": {
		"name": "Wuqiong Zhao",
		"email": "me@wqzhao.org",
		"url": "https://wqzhao.org"
	},
	"license": "MIT License",
	"keywords": ["Channel Estimation", "Millimeter Wave", "Simulation", "mmWave"],
	"repository": {
		"type": "git",
		"url": "https://github.com/mmcesim/mmcesim-vscode"
	},
	"engines": {
		"vscode": "^1.65.0"
	},
	"categories": [
		"Programming Languages"
	],
	"activationEvents": [
        "onCommand:mmcesim.viewWeb"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "mmcesim.viewWeb",
				"title": "mmCEsim Website"
			}
		],
		"languages": [
			{
				"id": "cpp",
				"extensions": [
					".mmcesim-cpp"
				],
				"aliases": ["mmCEsim C++"]
			},
			{
				"id": "python",
				"extensions": [
					".mmcesim-py"
				],
				"aliases": ["mmCEsim Python"]
			},
			{
				"id": "matlab",
				"extensions": [
					".mmcesim-m"
				],
				"aliases": ["mmCEsim Matlab"]
			},
			{
				"id": "yaml",
				"extensions": [
					".sim",
					".mmcesim-sim"
				],
				"aliases": ["mmCEsim"]
			},
			{
				"id": "mmcesim-alg",
				"extensions": [
					".alg", ".mmcesim-alg"
				],
				"aliases": ["mmCEsim ALG"]
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.71.0",
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^5.1.0",
		"@typescript-eslint/parser": "^5.1.0",
		"eslint": "^8.1.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.3",
		"typescript": "^4.4.4",
		"@vscode/test-electron": "^1.6.2"
	}
}
